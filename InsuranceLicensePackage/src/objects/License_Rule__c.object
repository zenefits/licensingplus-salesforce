    <validationRules>
        <fullName>Field_Value_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
  RecordType.Name = &apos;Regular Filter Rule&apos;, 
  IF(
    OR(
      ISBLANK(Field__c),
      AND(
        ISBLANK(Value__c),
        NOT(ISPICKVAL(Operator__c, &apos;not blank&apos;)) 
      )
    ), 
    true, 
    false
  ), 
  false
)</errorConditionFormula>
        <errorMessage>For Regular Filter Rule, you must specify both field and value, unless operator is set to &apos;not blank&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>License_Field_Field_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
  RecordType.Name = &apos;License Comparison Rule&apos;,
    IF(
      OR(
        ISBLANK(Field__c),
        ISBLANK(License_Field__c)
      ), true, false), false)</errorConditionFormula>
        <errorMessage>For License Comparison Rule or License State Rule, you must set field and license field.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>License_Field_Value_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
    RecordType.Name = &apos;License Filter Rule&apos;,
    IF( 
       OR( 
          ISBLANK(License_Field__c),
          AND(
            ISBLANK(Value__c), 
            NOT(ISPICKVAL(Operator__c, &apos;not blank&apos;)) 
          )
      ), true, false), false)</errorConditionFormula>
        <errorMessage>For License Filter Rule you must specify both value and license field, unless operator is set to &apos;not blank&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>License_State_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
  RecordType.Name = &apos;License State Rule&apos;,
    IF(
      OR(
        ISBLANK(Field__c),
        LOWER(License_Field__c)&lt;&gt;&apos;licensingplus__state__c&apos;,
        TEXT(Operator__c)&lt;&gt;&apos;equals&apos;
      ), true, false), false)</errorConditionFormula>
        <errorMessage>For License state rule, you must specify field and set license field to &apos;LicensingPlus__State__c&apos; and operator to &apos;equals&apos;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Resident_License_Rule</fullName>
        <active>true</active>
        <errorConditionFormula>IF( 
    RecordType.Name = &apos;Resident License Rule&apos;,
    IF( 
       OR( 
          LOWER(Value__c)&lt;&gt;&apos;resident state&apos;, 
          License_Field__c&lt;&gt;&apos;RecordType.Name&apos;,
          TEXT(Operator__c)&lt;&gt;&apos;equals&apos;
      ), true, false), false)</errorConditionFormula>
        <errorMessage>Resident License Rule has preset value as follows: Value=&apos;Resident State&apos;, License Field=&apos;RecordType.Name&apos;, Operator=&apos;equals&apos;, please do not change these field values.</errorMessage>
    </validationRules>
